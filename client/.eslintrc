{
  "root": true,
  "parser": "@typescript-eslint/parser",
  "parserOptions": {
    "emaVersion": 2020,
    "sourceType": "module"
  },
  "plugins": ["@typescript-eslint"],
  "extends": [
    "eslint:recommended",
    "plugin:@typescript-eslint/eslint-recommended",
    "plugin:@typescript-eslint/recommended"
  ],
  "rules": {
    "no-console": "warn",
    "@typescript-eslint/no-explicit-any": "off"
  },
  "env": {
    "node": true,
    "amd": true,
    "browser": true
  }
}
