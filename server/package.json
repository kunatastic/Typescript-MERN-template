{
  "name": "kunatastic-backend-template",
  "version": "1.0.0",
  "description": "backend template by kunatastic using node express and mongoDB",
  "main": "index.js",
  "scripts": {
    "build": "tsc -p .",
    "clean": "rm -rf dist",
    "devStart": "nodemon src/index.ts",
    "format": "prettier --ignore-path .gitignore --write \"**/*.+(js|ts|json)\"",
    "lint": "eslint --ignore-path .eslintignore --ext .js,.ts .",
    "start": "tsc -p . && node dist/index.js",
    "prepare": "cd .. && husky install"
  },
  "keywords": [
    "template",
    "nodejs",
    "expressjs",
    "typescript"
  ],
  "author": "kunatastic <kunatastic@gmail.com>",
  "license": "MIT",
  "dependencies": {
    "bcryptjs": "^2.4.3",
    "cookie-parser": "^1.4.6",
    "cors": "^2.8.5",
    "express": "^4.17.1",
    "helmet": "^4.6.0",
    "joi": "^17.5.0",
    "jsonwebtoken": "^8.5.1",
    "mongoose": "^6.1.1",
    "morgan": "^1.10.0"
  },
  "devDependencies": {
    "@types/cookie-parser": "^1.4.2",
    "@types/express": "^4.17.13",
    "@typescript-eslint/eslint-plugin": "^5.6.0",
    "@typescript-eslint/parser": "^5.6.0",
    "dotenv": "^10.0.0",
    "eslint": "^8.4.1",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^4.0.0",
    "husky": "^7.0.4",
    "nodemon": "^2.0.15",
    "prettier": "^2.5.1",
    "ts-node": "^10.4.0",
    "typescript": "^4.5.3"
  }
}
